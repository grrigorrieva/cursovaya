<Window x:Class="cursovaya.FromFileWnd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cursovaya"
        mc:Ignorable="d"
        Title="FromFileWnd" Height="200" Width="350">
    <Grid Background="Red">
        <TextBox x:Name="Name" HorizontalAlignment="Left" Margin="30,57,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120">
            <UIElement.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </UIElement.RenderTransform>
        </TextBox>
        <TextBox x:Name="Categr" HorizontalAlignment="Left" Margin="193,57,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Ok" HorizontalAlignment="Left" Margin="50,92,0,0" VerticalAlignment="Top" Height="37" Width="80" Click="Button_Click"/>
        <Button Content="cancel" HorizontalAlignment="Left" Margin="218,93,0,0" VerticalAlignment="Top" Height="36" Width="70" Click="Button_Click_1"/>

    </Grid>
</Window>
