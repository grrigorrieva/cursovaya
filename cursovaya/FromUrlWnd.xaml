<Window x:Class="cursovaya.FromUrlWnd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cursovaya"
        mc:Ignorable="d"
        Title="FromUrlWnd" Height="300" Width="500">
    <Grid Background="#FF63378D">
        <TextBox x:Name="url" HorizontalAlignment="Center" Height="38" Margin="0,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="480"/>
        <TextBox x:Name="Name" HorizontalAlignment="Left" Height="30" Margin="93,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="125"/>
        <TextBox x:Name="Categr" HorizontalAlignment="Left" Height="30" Margin="289,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="125"/>
        <Button Content="Ok" HorizontalAlignment="Left" Height="43" Margin="122,180,0,0" VerticalAlignment="Top" Width="112" Click="Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Height="43" Margin="275,180,0,0" VerticalAlignment="Top" Width="111" Click="Button_Click_1"/>

    </Grid>
</Window>
